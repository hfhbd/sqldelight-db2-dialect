CREATE TABLE foo(
  id INT GENERATED BY DEFAULT AS IDENTITY NOT NULL,
  name VARCHAR(255) NOT NULL
);

getAll:
SELECT * FROM foo;

new:
INSERT INTO foo VALUES ?;

create:
INSERT INTO foo (id, name) VALUES ?;

embedded:
SELECT * INTO :foo, :a FROM foo;

set:
SET :foo = 42;

setSelect:
SET :foo = SELECT * FROM foo;
