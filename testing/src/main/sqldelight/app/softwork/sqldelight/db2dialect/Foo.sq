CREATE TABLE foo(
  id INT GENERATED BY DEFAULT AS IDENTITY NOT NULL,
  id2 INT DEFAULT 42 NOT NULL,
  t TIMESTAMP(6) DEFAULT CURRENT TIMESTAMP NOT NULL,
  name VARCHAR(255) NOT NULL
);

getCurrentDate:
SELECT CURRENT DATE, CURRENT_DATE FROM foo;

getAll:
SELECT * FROM foo;

new:
INSERT INTO foo VALUES ?;

create:
INSERT INTO foo (id, name) VALUES ?;

embedded:
SELECT * INTO :foo, :a FROM foo;
